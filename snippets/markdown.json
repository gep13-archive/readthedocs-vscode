{
    "Attention Callout - Markdown": {
            "prefix": "rtdAttention",
            "body": [
                "<div class=\"admonition attention\">",
                "\t<p class=\"first admonition-title\">${1:Attention}</p>",
                "\t<p class=\"last\">",
                "\t\t${2:Enter Callout Text Here}",
                "\t</p>",
                "</div>"
            ],
            "description": "ReadTheDocs Attention Callout - Markdown"
        },
